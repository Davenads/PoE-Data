# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here

# Guild IDs
DISCORD_GUILD_ID_TEST=your_test_server_id_here
DISCORD_GUILD_ID_PROD=your_production_server_id_here

# Redis Configuration (WSL-Ubuntu)
# Get your WSL IP with: wsl hostname -I
# Configure Redis bind: wsl bash -c "redis-cli -a 'YOUR_PASSWORD' CONFIG SET bind '0.0.0.0'"
REDIS_HOST=172.19.187.251
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Bot Configuration
DEFAULT_LEAGUE=Rise of the Abyssal
UPDATE_INTERVAL_MINUTES=5
ENABLE_AUTOCOMPLETE=true
CHART_CACHE_DURATION=600

# Environment
NODE_ENV=development

# Logging
LOG_LEVEL=info

# Rate Limiting
POE_NINJA_MAX_REQUESTS=12
POE_NINJA_WINDOW_MINUTES=5
